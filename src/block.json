{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "markonikolas/post-meta",
	"version": "0.1.0",
	"title": "Post Meta",
	"category": "text",
	"icon": "smiley",
	"description": "Post meta block that displays artist profile information.",
	"example": {
		"attributes": {
			"selectedMeta": "_post_stats_likes"
		}
	},
	"supports": {
		"html": false,
		"multiple": true
	},
	"attributes": {
		"selectedMeta": {
			"type": "string",
			"default": "_post_stats_likes"
		}
	},
	"usesContext": ["postId", "postType"],
	"textdomain": "post-meta",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
